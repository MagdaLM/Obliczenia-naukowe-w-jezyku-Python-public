x = 5
x == 5 and 3                  # 3     -> ponieważ 'True and Anything' returns 'Anything', but not converted to 'bool'
x == 4 and 3                  # False -> ponieważ 'False and Anything' returns 'False', 'Anything' is not checked
3 and x == 5                  # True  -> ponieważ 'True or Anything' returns 'True', 'Anything' is not checked
3 and x == 4                  # False -> ponieważ 'False and Anything' returns 'False', 'Anything' is not checked

isinstance(True, int)         # True  -> ponieważ 'True' and 'False' are instances of the 'bool' class which is inherited from 'int'.
isinstance(True, bool)        # True  -> ponieważ 'True' and 'False' are instances of the 'bool' class which is inherited from 'int'.
